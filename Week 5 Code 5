#include<stdio.h>

int main(){
    int count, a, n=5, inc=1;

    for(count=1; count <= 9; count++){
        //printf("%d\n", n);
        for(a=1; a<=n; a++){
            printf("*");
        }
        n+=inc;
        if(n == 9){
            inc=-1;
        }
        puts("");
    }
    return 0;
}
